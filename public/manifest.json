{
  "manifest_version": 3,
  "name": "Supreme trobot",
  "description": "Automate changing prices on local marketplace sites",
  "version": "1.0.0",
  "action": {
    "default_title": "Supreme trobot",
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["https://allegrolokalnie.pl/*", "https://olx.pl/*"],
  "content_scripts": [
    {
      "matches": ["https://allegrolokalnie.pl/*"],
      "js": ["./marketplaces/allegro.js"]
    },
    {
      "matches": ["https://olx.pl/*"],
      "js": ["./marketplaces/olx.js"]
    }
  ]
}
