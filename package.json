    "@testing-library/svelte": "^3.0.0",
    "@types/chai": "^4.2.13",
    "@types/chrome": "0.0.134",
    "@types/mocha": "^8.2.0",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.12.0",
    "chai": "^4.2.0",
    "concurrently": "^6.0.0",
